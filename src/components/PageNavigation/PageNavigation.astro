---
interface Props {
    links: [string];
    logo: {
        [key: string]: string;
    };
    title: string;
    style: string;
}

import BasicFLoatingNav from "@components/PageNavigation/Navigation/BasicFloatingNav/Nav.astro";
import FloatingPill from "@components/PageNavigation/FloatingPill/FloatingPill.astro";
const props = Astro.props;
---

{props.style === "floating" && <BasicFLoatingNav data={props} />}
{props.style === "pill" && <FloatingPill data={props} />}
