---
interface Props {
    links: [string];
    logo: {
        [key: string]: string;
    };
    title: string;
    style: string;
    hero_style: string;
}

import BasicFLoatingNav from "@components/PageNavigation/Navigation/FixedNav/FixedNav.astro";
import FloatingPill from "@components/PageNavigation/FloatingPill/FloatingPill.astro";
import StickyNav from "@components/PageNavigation/StickyNav/StickyNav.astro";
const props = Astro.props;
let fixedMenu = false;
if (props.hero_style !== "two square") {
    fixedMenu = true;
}
---
<!-- {props.hero_style === "floating" && <BasicFLoatingNav data={props} />}
{props.hero_style === "two square" && <StickyNav data={props} />}
{props.hero_style === "pill" && <FloatingPill data={props} />}
{props.hero_style !== "two square" && <BasicFLoatingNav data={props} />} -->
<StickyNav data={props} fixedMenu={props.hero_style !== "two square" ? true : false} />